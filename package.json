{
  "name": "kyou",
  "version": "1.0.0",
  "description": "Ku-You-Cun project for TypeScript/React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "4.4.4"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "clean": "rimraf dist/",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "dev:server": "cross-env NODE_ENV=development PORT=3000 nodemon server/node-server.js",
    "dev:client": "cross-env NODE_ENV=development nodemon server/node-client.js",
    "dev": "concurrently -r \"npm run dev:client\" \"npm run dev:server\"",
    "lint": "npm run lint-ts && npm run lint-js && npm run stylefmt && npm run lint-css ",
    "lint-css": "stylelint \"./src/**/*.css\"",
    "lint-ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\"",
    "lint-js": "eslint \"**/*.js\" --fix --ignore-path .gitignore",
    "stylefmt": "stylefmt -R src/",
    "postinstall": "npm run typings && npm run build",
    "start": "cross-env NODE_ENV=production PORT=3002 node server/node-server.js",
    "pretest": "npm run lint",
    "test": "tsc && ava --verbose",
    "test:watch": "concurrently \"tsc -w\"  \"ava --verbose --watch\"",
    "typings": "rimraf typings/ && typings install",
    "ghpage": "cross-env PUBLIC_PATH=gh npm run build && node ./scripts/gh-pages.js"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "starter",
    "seed"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ninemilli-song/kyou"
  },
  "bugs": {
    "url": "https://github.com/ninemilli-song/kyou/issues"
  },
  "dependencies": {
    "antd": "^2.9.0",
    "classnames": "^2.2.5",
    "express": "^4.13.4",
    "helmet": "^2.0.0",
    "http-proxy": "^1.13.2",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "^7.0.12",
    "@types/react": "^15.0.21",
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "ava": "^0.15.2",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.9.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-antd": "^0.4.0",
    "babel-preset-es2015": "^6.9.0",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.3",
    "concurrently": "^2.0.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "denodeify": "^1.2.1",
    "enzyme": "^2.2.0",
    "es5-shim": "^4.5.6",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "gh-pages": "^0.11.0",
    "history": "^3.0.0",
    "html-webpack-plugin": "^2.21.0",
    "immutable": "^3.8.1",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.2.1",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.5.2",
    "nodemon": "^1.9.2",
    "nyc": "^6.6.1",
    "object-assign": "^4.0.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.9.1",
    "postcss-modules-local-by-default": "^1.0.1",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^5.0.3",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.4.0",
    "redux-form": "^5.2.1",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.6.0",
    "redux-thunk": "^2.0.1",
    "require-hacker": "^2.1.3",
    "rimraf": "^2.4.3",
    "sass-loader": "^6.0.3",
    "sinon": "^1.17.3",
    "source-map-loader": "^0.2.1",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "stylefmt": "^4.0.0",
    "stylelint": "^7.0.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "ts-babel-node": "^1.0.0",
    "ts-loader": "^2.0.3",
    "ts-node": "^2.0.0",
    "tslint": "^3.7.4",
    "tslint-loader": "^2.1.0",
    "typescript": "^2.2.2",
    "typings": "^2.1.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-split-by-path": "0.0.10",
    "whatwg-fetch": "^1.0.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "./test/helper/setup-test-env.js"
    ]
  }
}
